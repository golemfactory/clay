#!/Users/hawk/work/workspaces/golem-project/golem/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Automat','console_scripts','automat-visualize'
__requires__ = 'Automat'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Automat', 'console_scripts', 'automat-visualize')()
    )
