#!/Users/hawk/work/workspaces/golem-project/golem/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Twisted','console_scripts','cftp'
__requires__ = 'Twisted'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Twisted', 'console_scripts', 'cftp')()
    )
